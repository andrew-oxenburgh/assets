      "svg:png192": "svgexport ./public/site-logo.svg ./public/logo192.png png 100% 192:192",
      "svg:png256": "svgexport ./public/site-logo.svg ./public/logo512.png png 100% 512:512",
      "svg:png512": "svgexport ./public/site-logo.svg ./public/logo512.png png 100% 512:512",
